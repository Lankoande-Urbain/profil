{
   "builds": [
      {
         "src": "/package.json",
         "use": "@vercel/static-build",
         "config": {
            "distDir": "dist",
            "entrypoint": "/dist"
         }
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
}
